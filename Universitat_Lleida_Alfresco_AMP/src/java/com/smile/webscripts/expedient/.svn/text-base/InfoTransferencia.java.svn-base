package com.smile.webscripts.expedient;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

import org.alfresco.service.cmr.repository.NodeRef;

public class InfoTransferencia {
	private Map<NodeRef, List<NodeRef>> infoSignatures = new HashMap<NodeRef, List<NodeRef>>();
	private Map<NodeRef, NodeRef> signaturesDmRm = new HashMap<NodeRef, NodeRef>();
	private NodeRef rootRm = null;
	private String foliacio = "";

	public Map<NodeRef, List<NodeRef>> getInfoSignatures() {
		return infoSignatures;
	}
	public void setInfoSignatures(Map<NodeRef, List<NodeRef>> infoSignatures) {
		this.infoSignatures = infoSignatures;
	}
	public Map<NodeRef, NodeRef> getSignaturesDmRm() {
		return signaturesDmRm;
	}
	public void setSignaturesDmRm(Map<NodeRef, NodeRef> signaturesDmRm) {
		this.signaturesDmRm = signaturesDmRm;
	}
	public NodeRef getRootRm() {
		return rootRm;
	}
	public void setRootRm(NodeRef rootRm) {
		this.rootRm = rootRm;
	}
	public String getFoliacio() {
		return foliacio;
	}
	public void setFoliacio(String foliacio) {
		this.foliacio = foliacio;
	}
}
