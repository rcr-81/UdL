<?xml version="1.0" encoding="UTF-8"?>

<response>
	<service>esborrarGrup</service>
	<username>${username}</username>
	<success>${success}</success>
	<#if error?exists>   
		<error>${error}</error>
	</#if>
	<#if success=="true">   
		<grup>
			<name>${nomGrup}</name>
			<id>${idGrup}</id>			
		</grup>			
	</#if>
</response>